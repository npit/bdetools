//> DESCRIBE KEYSPACE BDE;
// =======================================
 CREATE KEYSPACE bde WITH replication = {'class': 'SimpleStrategy',

'replication_factor': '1'}  AND durable_writes = true;

 CREATE TYPE bde.geolocation (
     latitude double,
     longitude double
 );

 CREATE TYPE bde.tplace (
     id text,
     full_name text,
     name text,
     street_address text,
     country_code text,
     country text,
     place_type text,
     url text,
     bounding_box_type text,
     geometry_type text,
     bounding_box text
 );

 CREATE TABLE bde.twitter_posts_per_place (
     place_literal text,
     created_at bigint,
     post_id bigint,
     account_name text,
     language text,
     place frozen<tplace>,
     tweet text,
     url text,
     PRIMARY KEY (place_literal, created_at, post_id)
 ) WITH CLUSTERING ORDER BY (created_at DESC, post_id DESC)
     AND bloom_filter_fp_chance = 0.01
     AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
     AND comment = ''
     AND compaction = {'class':
 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
     AND compression = {'sstable_compression':
 'org.apache.cassandra.io.compress.LZ4Compressor'}
     AND dclocal_read_repair_chance = 0.1
     AND default_time_to_live = 0
     AND gc_grace_seconds = 864000
     AND max_index_interval = 2048
     AND memtable_flush_period_in_ms = 0
     AND min_index_interval = 128
     AND read_repair_chance = 0.0
     AND speculative_retry = '99.0PERCENTILE';

 CREATE TABLE bde.news_articles_per_crawled_date (
     year_month_day_literal text,
     crawled bigint,
     entry_url text,
     clean_text text,
     crawl_id bigint,
     feed_url text,
     language text,
     place_literal set<text>,
     published bigint,
     raw_text text,
     title text,
     PRIMARY KEY (year_month_day_literal, crawled, entry_url)
 ) WITH CLUSTERING ORDER BY (crawled DESC, entry_url ASC)
     AND bloom_filter_fp_chance = 0.01
     AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
     AND comment = ''
     AND compaction = {'class':
 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
     AND compression = {'sstable_compression':
 'org.apache.cassandra.io.compress.LZ4Compressor'}
     AND dclocal_read_repair_chance = 0.1
     AND default_time_to_live = 0
     AND gc_grace_seconds = 864000
     AND max_index_interval = 2048
     AND memtable_flush_period_in_ms = 0
     AND min_index_interval = 128
     AND read_repair_chance = 0.0
     AND speculative_retry = '99.0PERCENTILE';
 CREATE INDEX rssc_crawlid_idx ON bde.news_articles_per_crawled_date

(crawl_id);

 CREATE INDEX rssc_lang_idx ON bde.news_articles_per_crawled_date

(language);

 CREATE TABLE bde.twitter_log (
     engine_type text,
     engine_id bigint,
     ended bigint,
     started bigint,
     PRIMARY KEY (engine_type, engine_id)
 ) WITH CLUSTERING ORDER BY (engine_id DESC)
     AND bloom_filter_fp_chance = 0.01
     AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
     AND comment = ''
     AND compaction = {'class':
 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
     AND compression = {'sstable_compression':
 'org.apache.cassandra.io.compress.LZ4Compressor'}
     AND dclocal_read_repair_chance = 0.1
     AND default_time_to_live = 0
     AND gc_grace_seconds = 864000
     AND max_index_interval = 2048
     AND memtable_flush_period_in_ms = 0
     AND min_index_interval = 128
     AND read_repair_chance = 0.0
     AND speculative_retry = '99.0PERCENTILE';

 CREATE TABLE bde.events_per_place_literal (
     place_literal text,
     event_id text,
     description text,
     event_date text,
     event_source_urls map<text,text>,
     place_polygon text,
     title text,
     tweet_post_ids map<bigint,text>,
     PRIMARY KEY (place_literal, event_id)
 ) WITH CLUSTERING ORDER BY (event_id DESC)
     AND bloom_filter_fp_chance = 0.01
     AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
     AND comment = ''
     AND compaction = {'class':
 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
     AND compression = {'sstable_compression':
 'org.apache.cassandra.io.compress.LZ4Compressor'}
     AND dclocal_read_repair_chance = 0.1
     AND default_time_to_live = 0
     AND gc_grace_seconds = 864000
     AND max_index_interval = 2048
     AND memtable_flush_period_in_ms = 0
     AND min_index_interval = 128
     AND read_repair_chance = 0.0
     AND speculative_retry = '99.0PERCENTILE';

 CREATE TABLE bde.twitter_post (
     post_id bigint,
     language text,
     account_name text,
     coordinates text,
     created_at bigint,
     external_links set<text>,
     followers_when_published bigint,
     place text,
     retweet_count bigint,
     tweet text,
     url text,
     PRIMARY KEY (post_id, language)
 ) WITH CLUSTERING ORDER BY (language ASC)
     AND bloom_filter_fp_chance = 0.01
     AND caching = '{"keys":"ALL", "rows_per_partition":"NONE"}'
     AND comment = ''
     AND compaction = {'class':
 'org.apache.cassandra.db.compaction.SizeTieredCompactionStrategy'}
     AND compression = {'sstable_compression':
 'org.apache.cassandra.io.compress.LZ4Compressor'}
     AND dclocal_read_repair_chance = 0.1
     AND default_time_to_live = 0
     AND gc_grace_seconds = 864000
     AND max_index_interval = 2048
     AND memtable_flush_period_in_ms = 0
     AND min_index_interval = 128
     AND read_repair_chance = 0.0
     AND speculative_retry = '99.0PERCENTILE';
 CREATE INDEX post_account_name_idx ON bde.twitter_post (account_name);

